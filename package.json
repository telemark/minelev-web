  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "tfk",
    "Digitalisering",
    "MinElev"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "Joan Jett",
    "versionVideoUrl": "https://www.youtube.com/watch?v=46G1nRdCOio",
    "systemName": "MinElev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/minelev-web.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/minelev-web/issues"
  },
  "homepage": "https://github.com/telemark/minelev-web",
  "now": {
    "alias": [
      "web.minelev.t-fk.win"
    ],
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.18.2",
