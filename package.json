{
  "name": "minelev-web",
  "version": "1.7.1",
  "description": "web frontend for MinElev",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "now-deploy": "npm test && now --npm && now alias",
    "start": "node standalone.js",
    "dev": "NODE_ENV=development node standalone.js"
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "tfk",
    "Digitalisering",
    "MinElev"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "Joan Jett",
    "versionVideoUrl": "https://www.youtube.com/watch?v=46G1nRdCOio",
    "systemName": "MinElev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/minelev-web.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/minelev-web/issues"
  },
  "homepage": "https://github.com/telemark/minelev-web",
  "now": {
    "alias": [
      "www.minelev.no"
    ],
    "type": "npm",
    "dotenv": "production.env"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "coveralls": "3.0.0",
    "nsp": "2.8.1",
    "nyc": "11.2.1",
    "standard": "10.0.3"
  },
  "dependencies": {
    "axios": "0.16.2",
    "form-data": "2.3.1",
    "get-skole-aar": "1.0.0",
    "good": "7.3.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.2",
    "handlebars": "4.0.10",
    "hapi": "16.6.2",
    "hapi-auth-cookie": "7.0.0",
    "hapi-auth-jwt2": "7.3.0",
    "inert": "4.2.1",
    "jsonwebtoken": "8.0.1",
    "simple-encryptor": "1.1.0",
    "tfk-saksbehandling-minelev-templates": "1.5.1",
    "tfk-saksbehandling-schools-info": "2.0.12",
    "vision": "4.1.1",
    "winston": "2.4.0",
    "winston-papertrail": "1.0.5",
    "yar": "8.1.2"
  }
}
