{
  "name": "minelev-web",
  "version": "1.0.0",
  "description": "web frontend for MinElev",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
